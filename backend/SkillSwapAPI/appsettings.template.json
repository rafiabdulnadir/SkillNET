{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=SkillSwapDB;Username=postgres;Password=[YOUR_PASSWORD]"
  },
  "JwtSettings": {
    "SecretKey": "[YOUR_SECRET_KEY]",
    "Issuer": "SkillSwapAPI",
    "Audience": "SkillSwapUsers",
    "ExpiryMinutes": "1440"
  },
  "AppSettings": {
    "FrontendUrl": "http://localhost:3000"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "AllowedHosts": "*"
}
